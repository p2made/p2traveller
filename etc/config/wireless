config wifi-device 'radio0'
	option type                 'mac80211'
	option channel              '11'
	option hwmode               '11g'
	option path                 'platform/10300000.wmac'
	option htmode               'HT20'

config wifi-iface 'default_radio0'
	option ssid                 'Broadcast-WiFi'
	option key                  'Broadcast-WiFi-Password'
	option device               'radio0'
	option network              'lan'
	option mode                 'ap'
	option encryption           'psk2'

config wifi-device 'radio1'
	option type                 'mac80211'
	option channel              '36'
	option hwmode               '11a'
	option path                 'pci0000:00/0000:00:00.0/0000:01:00.0'
	option htmode               'VHT80'

config wifi-iface 'default_radio1'
	option ssid                 'Broadcast-WiFi-5G'
	option key                  'Broadcast-WiFi-Password'
	option device               'radio1'
	option network              'lan'
	option mode                 'ap'
	option encryption           'psk2'


config wifi-iface 'trm_uplink4'
	option ssid                 'First-Public-WiFi'
	option key                  'First-Public-WiFi-Password'
	option device               'radio0'
	option network              'trm_wwan'
	option mode                 'sta'
	option encryption           'psk2+ccmp'
	option disabled             '1'

config wifi-iface 'trm_uplink5'
	option ssid                 'First-Public-WiFi-5G'
	option key                  'First-Public-WiFi-Password'
	option device               'radio1'
	option network              'trm_wwan'
	option mode                 'sta'
	option encryption           'psk2+ccmp'
	option disabled             '1'

config wifi-iface 'trm_uplink6'
	option ssid                 'Next-Public-WiFi'
	option key                  'Next-Public-WiFi-Password'
	option device               'radio0'
	option network              'trm_wwan'
	option mode                 'sta'
	option encryption           'psk2+ccmp'
	option disabled             '1'

config wifi-iface 'trm_uplink7'
	option ssid                 'Next-Public-WiFi-5G'
	option key                  'Next-Public-WiFi-Password'
	option device               'radio1'
	option network              'trm_wwan'
	option mode                 'sta'
	option encryption           'psk2+ccmp'
	option disabled             '0'
